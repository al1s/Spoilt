@model IEnumerable<Spoilt.Models.Movie>
@{
    ViewData["Title"] = "Home";
    Layout = "_Layout";
}
<h1>SPOILT!</h1>
<h2>The site that lets you view and contribute movie spoilers</h2>

<p>Consider this your Spoiler Warning</p>

<ul>
    @foreach(var item in Model)
    {
        <li>
            @Html.DisplayFor(modelItem => item.Title)
        </li>
    }
</ul>
