@model IEnumerable<Spoilt.Models.Movie>
@{
    ViewData["Title"] = "Home";
    Layout = "_Layout";
}

@{
    await Html.RenderPartialAsync("/Views/Shared/_SearchBar.cshtml");
}
<div class="row">
    @foreach (var item in Model)
    {
        await Html.RenderPartialAsync("../Shared/_MovieCard.cshtml", item);
    }
</div>
